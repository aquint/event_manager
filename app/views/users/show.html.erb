<div class="container">
	<div class="row show-user">
		<div class="col-md-12">
			<h1></h1>
			<h2><%= @user.email %></h2>
			<ul>
				<% @created_events.each do |created_event| %>
					<li>
						<%= created_event.name %>
						<%= created_event.description %>
						<%= created_event.date %>
					</li>
				<% end %>
			</ul>
		</div>
	</div>
</div>